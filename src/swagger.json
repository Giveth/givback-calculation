{
  "swagger": "2.0",
  "basePath": "/",
  "paths": {
    "/calculate-givback": {
      "get": {
        "parameters": [
          {
            "name": "startDate",
            "type": "string",
            "default": "2021/12/24-16:00:00",
            "in": "query"
          },
          {
            "name": "endDate",
            "type": "string",
            "default": "2022/01/07-16:00:00",
            "in": "query"
          },
          {
            "name": "givPrice",
            "type": "string",
            "default": "1",
            "in": "query"
          },
          {
            "name": "givAvailable",
            "type": "string",
            "default": "1000000",
            "in": "query"
          },
          {
            "name": "givMaxFactor",
            "type": "string",
            "default": "0.75",
            "in": "query"
          },
          {
            "name": "nrGIVAddress",
            "type": "string",
            "default": "0xba4858a7cDCA54698A28947F2dF874990bB724f1",
            "in": "query"
          },
          {
            "name": "tokenDistroAddress",
            "type": "string",
            "default": "0xf11aC05c97F8845f6b974e9e327129d36683cC90",
            "in": "query"
          },
          {
            "name": "distributorAddress",
            "type": "string",
            "default": "0xb85431ead6471b6fb83afe797b03578c84b24af6",
            "in": "query"
          },
          {
            "name": "maxAddressesPerFunctionCall",
            "type": "string",
            "default": "200",
            "in": "query"
          },
          {
            "name": "download",
            "type": "string",
            "description": "If you want to see json here set no, but if you want to download a file set yes",
            "default": "no",
            "enum": ["yes","no"],
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }

        }
      }
    },
    "/eligible-donations": {
      "get": {
        "parameters": [
          {
            "name": "startDate",
            "type": "string",
            "default": "2021/12/24-16:00:00",
            "in": "query"
          },
          {
            "name": "endDate",
            "type": "string",
            "default": "2022/01/07-16:00:00",
            "in": "query"
          },
          {
            "name": "download",
            "type": "string",
            "description": "If you want to see json here set no, but if you want to download a file set yes",
            "default": "no",
            "enum": ["yes","no"],
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }

        }
      }
    },
    "/donations-leaderboard": {
      "get": {
        "parameters": [
          {
            "name": "startDate",
            "type": "string",
            "default": "2021/09/01-00:00:00",
            "in": "query"
          },
          {
            "name": "endDate",
            "type": "string",
            "default": "2021/12/31-00:00:00",
            "in": "query"
          },
          {
            "name": "total",
            "type": "number",
            "default": "10",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }

        }
      }
    },
    "/givPrice": {
      "get": {
        "description":"Giv price on xdai and mainnet network ( GIV/ETH comes from sushiswap/uniswap pool, ETH/USD comes from crypto compare API)",
        "parameters": [
          {
            "in": "query",
            "name": "network",
            "default": "xdai",
            "enum": [
              "xdai",
              "mainnet"
            ]
          },
          {
            "in": "query",
            "name": "txHash",
            "description": "txHash on xDai"
          },
          {
            "in": "query",
            "name": "blockNumber",
            "description": "blockNumber on xdai"
          }
        ],
        "responses": {
          "200": {

          }
        }
      }
    }

  }
}
